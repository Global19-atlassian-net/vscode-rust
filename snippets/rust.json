{
    "extern crate": {
        "prefix": "extern crate",
        "body": [
            "extern crate ${1:name};$0"
        ],
        "description": "Insert extern crate"
    },
    "for": {
        "prefix": "for",
        "body": [
            "for $1 in $2 {",
            "\t$0",
            "}"
        ],
        "description": "Insert for loop"
    },
    "macro_rules": {
        "prefix": "macro_rules",
        "body": [
            "macro_rules! $1 {",
            "\t($2) => {",
            "\t\t$0",
            "\t};",
            "}"
        ],
        "description": "Insert macro_rules!"
    },
    "if let Option": {
        "prefix": "if let",
        "body": [
            "if let Some(${1:foo}) = ${1:foo} {",
            "\t${0:${1:foo}.}",
            "}"
        ],
        "description": "Unwrap Option with if let"
    },
    "spawn": {
        "prefix": [
            "thread_spawn",
            "spawn"
        ],
        "body": [
            "std::thread::spawn(move || {",
            "\t$1",
            "})$0"
        ],
        "description": "Wrap code in thread::spawn"
    },
    "derive": {
        "prefix": "derive",
        "body": [
            "#[derive(${1})]"
        ],
        "description": "#[derive(…)]"
    },
    "cfg": {
        "prefix": "cfg",
        "body": [
            "#[cfg(${1})]"
        ],
        "description": "#[cfg(…)]"
    },
    "test": {
        "prefix": "test",
        "body": [
            "#[test]",
            "fn ${1:name}() {",
            "    ${2:unimplemented!();}",
            "}"
        ],
        "description": "#[test]"
    }
}
